<form version="1.1" hideExport="true">
  <label>Overview</label>
    <search>
<query>| makeresults | eval name_app= if(like("$env:app$","%holistic%"),mvindex(split("$env:app$","_holistic"),0)."_hl",mvindex(split("$env:app$","_e2e"),0)."_e2e")</query>
<progress>
<set token="name_app">$result.name_app$</set>
</progress>
</search>
  
  <init>
    <set token="word_infra">Loading...</set>
    <set token="word_db">Loading...</set>
    <set token="word_perf">Loading...</set>
    <set token="word_itsm">Loading...</set>
  </init>
  <search>
    <query>
      
      | inputlookup armada_list_servers.csv
| stats first(status) as Status,first(icon_value) as icon_value,first(color_value) as color_value by Hostname
| sort Hostname
| eval Range=if(Status="Offline","Elevated","Low") 
| stats values(Status) as Status
    </query>
    <progress>
      <condition match="like('result.Status' ,&quot;Offline&quot;)">
        <set token="icon_infra">icon_error.PNG</set>
        <set token="color_infra">#FF0000;</set>
        <set token="word_infra">ERROR</set>
        <set token="size_infra">15.5%</set>
      </condition>
      <condition match="like('result.Status' ,&quot;Online&quot;)">
        <set token="icon_infra">icon_healthy.PNG</set>
        <set token="color_infra">#8aa86d;</set>
        <set token="word_infra">HEALTHY</set>
        <set token="size_infra">21.5%</set>
      </condition>
    </progress>
  </search>
  <search>
    <query>
      
   index=sag_database_normal O1P18698DG1 sourcetype="database:oracle:failed_jobs" 
| stats sum(FAILURE_COUNT) as Failure by INSTANCE_NAME 
| eval status=if(Failure=0,"success","failed")
| fields status
    </query>
    <progress>
      <condition match="NOT like('result.status' ,&quot;success&quot;)">
        <set token="icon_db">icon_error.PNG</set>
        <set token="color_db">#FF0000;</set>
        <set token="word_db">ERROR</set>
        <set token="size_db">15.5%</set>
      </condition>
      <condition match="like('result.status' ,&quot;success&quot;)">
        <set token="icon_db">icon_healthy.PNG</set>
        <set token="color_db">#8aa86d;</set>
        <set token="word_db">HEALTHY</set>
        <set token="size_db">21.5%</set>
      </condition>
    </progress>
  </search>
  <search>
    <query>
index=sag_common_hpbsm NAME_APPLICATION="armada"  AVAILABILITY="0"
|  stats latest(AVAILABILITY) as AVAILABILITY, latest(RESPONSE_TIME) as RESPONSE_TIME by NAME_APPLICATION, NAME_BTF, NAME_LOCATION,NAME_TRANSACTION
|  rename NME_BTF as "Business process", NAME_TRANSACTION as "Transaction name"
 | stats count    | eval status=if( count=0, "check","Failed") , color = if( count=0, "#53a051","#dc4e41") | table status
 | stats values(status) as status
    </query>
    <earliest>$timepicker.earliest$</earliest>
    <latest>$timepicker.latest$</latest>
    <progress>
      <condition match="like('result.status' ,&quot;%Failed%&quot;)">
        <set token="icon_perf">icon_error.PNG</set>
        <set token="color_perf">#FF0000;</set>
        <set token="word_perf">ERROR</set>
        <set token="size_perf">15.5%</set>
      </condition>
      <condition match="NOT like('result.status' ,&quot;%Failed%&quot;)">
        <set token="icon_perf">icon_healthy.PNG</set>
        <set token="color_perf">#8aa86d;</set>
        <set token="word_perf">HEALTHY</set>
        <set token="size_perf">21.5%</set>
      </condition>
    </progress>
  </search>
  <search>
    <query>
|inputlookup incident_details_so|dedup modification_time,number  | search "Service Offering"="armada" 
| search status!="Closed" AND status!="Resolved"
| eval count=0
 | stats dc(number) as count
    </query>
    <progress>
      <condition match="'result.count' &gt;0">
        <set token="icon_itsm">icon_error.PNG</set>
        <set token="color_itsm">#FF0000;</set>
        <set token="word_itsm">ERROR</set>
        <set token="size_itsm">15.5%</set>
      </condition>
      <condition match="like('result.count' ,&quot;0&quot;)">
        <set token="icon_itsm">icon_healthy.PNG</set>
        <set token="color_itsm">#8aa86d;</set>
        <set token="word_itsm">HEALTHY</set>
        <set token="size_itsm">21.5%</set>
      </condition>
    </progress>
  </search>
  <fieldset submitButton="false"></fieldset>
